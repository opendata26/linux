					 <&mmcc MMSS_MISC_AHB_CLK>,
					 <&mmcc MMSS_S0_AXI_CLK>,
					 <&mmcc MMSS_MMSSNOC_AXI_CLK>;
            clock-names = "mdp_core",
                      "iface",
                      "bus",
                      "byte",
                      "pixel",
                      "core",
                      "core_mmss",
		      "s0_axi",
		      "mmssnoc";

            phys = <&dsi_phy0>;
            phy-names ="dsi-phy";

            vdda-supply = <&pm8941_l2>;
